<h2><%= book.title %></h2>
<img height='150' width='100' src='<%= book.coverImageData %>'>
<div>Author: <%= author.name %></div>
<div>Publish date: <%= book.publishDate.toDateString() %></div>
<div>Page count: <%= book.pageCount %></div>
<div>Description: <%= book.description %></div>
<a href='/books/<%= book.id %>/edit' class='btn btn-primary'>Edit</a>
<%- include('../partials/deleteForm',{url:`/books/${book.id}`}) %>
<a href='/authors/<%= author.id %>' class='btn btn-primary'>View author</a>